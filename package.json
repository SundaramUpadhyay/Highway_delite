{
  "name": "highway-delite",
  "version": "1.0.0",
  "description": "Experience booking platform - Highway Delite",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run dev",
